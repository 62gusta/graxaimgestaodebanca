  object qrApostas: TSQLQuery
    IndexName = 'DEFAULT_ORDER'
    MaxIndexesCount = 4
    FieldDefs = <    
      item
        Name = 'Retorno'
        DataType = ftBCD
        Precision = 9
        Size = 2
      end    
      item
        Name = 'Lucro'
        DataType = ftBCD
        Precision = 9
        Size = 2
      end    
      item
        Name = 'Banca_Final'
        DataType = ftBCD
        Precision = 9
        Size = 2
      end    
      item
        Name = 'Cod_Aposta'
        DataType = ftLargeint
        Precision = -1
      end    
      item
        Name = 'Data'
        DataType = ftDate
        Precision = -1
      end    
      item
        Name = 'Selecao'
        DataType = ftBoolean
        Precision = -1
      end    
      item
        Name = 'Múltipla'
        DataType = ftBoolean
        Precision = -1
      end    
      item
        Name = 'Jogo'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'Odd'
        DataType = ftBCD
        Precision = 9
        Size = 2
      end    
      item
        Name = 'Valor_Aposta'
        DataType = ftBCD
        Precision = 9
        Size = 2
      end    
      item
        Name = 'Status'
        DataType = ftString
        Precision = -1
        Size = 255
      end>
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'SELECT'
      '    Apostas.Retorno,'
      '    Apostas.Lucro,'
      '    Apostas.Banca_Final,'
      '    Apostas.Cod_Aposta,'
      '    Apostas.Data,'
      '    Apostas."Selec." AS Selecao,'
      '    Apostas.Múltipla,'
      '    CASE'
      '        WHEN Apostas."Múltipla" = 0 THEN Competições.Competição || '': '' || Jogo.Mandante || '' X '' || Jogo.Visitante'
      '        WHEN Apostas."Múltipla" = 1 THEN ''Aposta Múltipla'''
      '    END AS Jogo,'
      '    Apostas.Odd,'
      '    Apostas.Valor_Aposta,'
      '    Apostas.Status'
      'FROM'
      '    Apostas'
      'LEFT JOIN'
      '    Mercados ON Apostas.Cod_Aposta = Mercados.Cod_Aposta'
      'LEFT JOIN'
      '    Jogo ON Mercados.Cod_Jogo = Jogo.Cod_Jogo'
      'INNER JOIN'
      '    Competições ON Jogo.Cod_Comp = Competições.Cod_Comp'
      'WHERE strftime(''%m'', Apostas.Data) = :mesSelec'
      'AND strftime(''%Y'', Apostas.Data) = :anoSelec'
      'GROUP BY Apostas.Cod_Aposta'
      'ORDER BY Apostas.Data, Apostas.Cod_Aposta;'
    )
    InsertSQL.Strings = (
      'INSERT INTO Apostas "Data", Competição_AP, Mandante, Visitante, Estratégia_Escolhida, Odd, Unidade, Valor_Aposta, Status) VALUES (:V1, V2, V3, V4, V5, V6, V7, V8, V9);'
      ''
    )
    UpdateSQL.Strings = (
      'UPDATE Apostas SET "Selec." = :Selecao, Data = :Data WHERE Cod_Aposta = :Cod_Aposta;'
    )
    DeleteSQL.Strings = (
      'DELETE FROM Apostas WHERE "Selec." = 1'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'mesSelec'
        ParamType = ptInput
      end    
      item
        DataType = ftUnknown
        Name = 'anoSelec'
        ParamType = ptInput
      end>
