  object qrMes: TSQLQuery
    IndexName = 'DEFAULT_ORDER'
    MaxIndexesCount = 4
    FieldDefs = <    
      item
        Name = 'Dia'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'SomaLucro'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'PorCentoLucro'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'Green'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'Red'
        DataType = ftString
        Precision = -1
        Size = 255
      end>
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'SELECT'
      '  strftime(''%d'', Apostas.Data) AS Dia,'
      ''
      '  ROUND('
      '    COALESCE(('
      '      SELECT SUM(A2.Lucro)'
      '      FROM Apostas AS A2'
      '      WHERE strftime(''%m'', A2.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '        AND strftime(''%Y'', A2.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '        AND strftime(''%d'', A2.Data) <= strftime(''%d'', Apostas.Data)'
      '    ), 0), 2) AS SomaLucro,'
      ''
      '  ROUND('
      '    CASE'
      '      WHEN COALESCE(('
      '        SELECT Valor_Inicial'
      '        FROM Banca AS valBanca'
      '        WHERE valBanca.Mês = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '          AND valBanca.Ano = (SELECT Ano FROM "Selecionar Mês e Ano")), 0) = 0'
      '      THEN 0'
      '      ELSE COALESCE(('
      '        SELECT SUM(A2.Lucro)'
      '        FROM Apostas AS A2'
      '        WHERE strftime(''%m'', A2.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '          AND strftime(''%Y'', A2.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '          AND strftime(''%d'', A2.Data) <= strftime(''%d'', Apostas.Data)'
      '      ), 0) / ('
      '        SELECT Valor_Inicial'
      '        FROM Banca AS valBanca'
      '        WHERE valBanca.Mês = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '          AND valBanca.Ano = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '      ) * 100'
      '    END, 2) AS PorCentoLucro,'
      ''
      '  IFNULL(('
      '    SELECT 1'
      '    FROM Apostas AS A3'
      '    WHERE strftime(''%m'', A3.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%Y'', A3.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%d'', A3.Data) = strftime(''%d'', Apostas.Data)'
      '    GROUP BY strftime(''%d'', A3.Data)'
      '    HAVING SUM(Lucro) > 0'
      '  ), 0) AS Green,'
      ''
      '  IFNULL(('
      '    SELECT 1'
      '    FROM Apostas AS A4'
      '    WHERE strftime(''%m'', A4.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%Y'', A4.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%d'', A4.Data) = strftime(''%d'', Apostas.Data)'
      '    GROUP BY strftime(''%d'', A4.Data)'
      '    HAVING SUM(Lucro) < 0'
      '  ), 0) AS Red'
      ''
      'FROM Apostas'
      'WHERE strftime(''%m'', Apostas.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '  AND strftime(''%Y'', Apostas.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      'GROUP BY Dia'
      'ORDER BY Dia;'
    )
    Params = <>
