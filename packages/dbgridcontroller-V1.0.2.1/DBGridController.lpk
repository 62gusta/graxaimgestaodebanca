<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="4">
    <PathDelim Value="\"/>
    <Name Value="DBGridController"/>
    <Type Value="RunAndDesignTime"/>
    <Author Value="Bernard Cincou P.Eng"/>
    <CompilerOptions>
      <Version Value="11"/>
      <PathDelim Value="\"/>
      <SearchPaths>
        <UnitOutputDirectory Value="lib\$(TargetCPU)-$(TargetOS)"/>
      </SearchPaths>
    </CompilerOptions>
    <Description Value="TDBGridController is a component with no dependencies
providing added fonctionnalities for the TDBGrid. "/>
    <License Value="MPL 2.0, LGPL "/>
    <Version Major="1" Release="2"/>
    <Files Count="2">
      <Item1>
        <Filename Value="dxDBgridController.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="dxDBGridController"/>
      </Item1>
      <Item2>
        <Filename Value="JsonTools.pas"/>
        <UnitName Value="JsonTools"/>
      </Item2>
    </Files>
    <RequiredPkgs Count="4">
      <Item1>
        <PackageName Value="DateTimeCtrls"/>
      </Item1>
      <Item2>
        <PackageName Value="LazControls"/>
      </Item2>
      <Item3>
        <PackageName Value="LCL"/>
      </Item3>
      <Item4>
        <PackageName Value="FCL"/>
      </Item4>
    </RequiredPkgs>
    <UsageOptions>
      <UnitPath Value="$(PkgOutDir)"/>
    </UsageOptions>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
    <CustomOptions Items="ExternHelp" Version="2">
      <_ExternHelp Items="Count"/>
    </CustomOptions>
  </Package>
</CONFIG>
